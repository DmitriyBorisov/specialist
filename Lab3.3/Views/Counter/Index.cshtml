@model Dictionary<string, int>

@{
    ViewData["Title"] = "Counters";
}

<h1>Counters</h1>

<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Key
            </td>
            <td>
                @item.Value
            </td>
        </tr>
}
    </tbody>
</table>
